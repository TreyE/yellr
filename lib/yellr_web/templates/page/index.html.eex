<div class="row resultcontainer">
<%= for branch <- @branches do %>
  <div class="card <%= branch_status_class(branch) %> floatcard">
    <div class="card-body">
      <h3 class="card-title"><%= branch.project %></h3>
      <h4 class="card-title"><%= branch.branch %></h4>
      <h4 class="card-title"><%= build_time(branch) %></h4>
      <%= unless branch.passing do %>
      <div class="card-text">
      <p class="lead"><%= contributor_list(branch) %></p>
      </div>
      <% end %>
    </div>
  </div>
<% end %>
</div>

<%= if broken_builds(@branches) do %>
  <audio autoplay preload>
    <source src="<%= Routes.static_path(@conn, "/media/willhelm.mp3") %>" type="audio/mpeg">
  </audio>
<% end %>