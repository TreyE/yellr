#!/bin/bash

build_result_val=1

if [[ "$TRAVIS_TEST_RESULT" == "0" ]]; then
  build_result_val="passing"
else
  build_result_val="failing"
fi

curl -H "Content-Type: application/json" -H "X-API-Key: ${BUILD_REPORT_API_KEY}" -X POST -d "{'project':'yellr','branch':'$TRAVIS_BRANCH','sha':'$TRAVIS_COMMIT','status':'$build_result_val'}" ${BUILD_REPORT_URL}
